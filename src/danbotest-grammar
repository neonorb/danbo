D_DEFCHAR(la, 'a')
D_DEFCHAR(lb, 'b')
D_DEFSWITCH(letter, la, lb)

D_DEFCHAR(openpar, '(')
D_DEFCHAR(closepar, ')')
D_DEFLITERAL(expression, D_U(openpar), D_N(nletter, letter), D_U(closepar))

D_DEFVARIABLE(variableexpressions, expression)

D_DEFMANY(manyexpressions, expression)

D_DEFOPTIONAL(optionala, la)